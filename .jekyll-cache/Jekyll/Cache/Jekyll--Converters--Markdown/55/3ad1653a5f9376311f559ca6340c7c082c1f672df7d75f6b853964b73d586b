void bunnle_sort(int arr[], int count)
{
	int temp;
	for (int i = 0; i &lt; count; i++)
	{
		for (int j = 0; j &lt; count - 1; j++)
		{
			if (arr[j] &gt; arr[j + 1])
			{
				temp = arr[j];
				arr[j] = arr[j + 1];
				arr[j + 1] = temp;
			}
		}
	}
}
int main50() {
	int numArr[10] = { 8,4,2,5,3,7,10,1,6,9 };</p>
